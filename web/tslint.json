{
    "extends": ["tslint:latest", "tslint-react", "tslint-config-standard"],
    "rules": {
        "quotemark": [true, "single"],
        "no-console": [true, "warn"],
        "arrow-return-shorthand": [false],
        "ter-indent": [true, 4],
        "semicolon": [true, "always"],
        "max-line-length": [true, 220]
    }
}